.img {
  width: 100%;
  height:100%;
  position: relative;
}
.dish1, .dish2 {
  position: absolute;
  top:0;
  border-radius: 20rpx;
  overflow: hidden;
  background-color: #fff;
}
.dish1 {
  left:10rpx;
}
.dish2 {
  right:10rpx;
}
.dish1 .bigImg,
.dish2 .bigImg {
  width:100%;
  background-color: rgb(227, 227, 227);
}
.dish1 .content,
.dish2 .content {
  width: 100%;
  height:110rpx;
}
.dish1 .dish-title,
.dish2 .dish-title {
  padding: 0 10rpx;
  font-weight: 700;
}
.dish1 .dish-info,
.dish2 .dish-info {
  font-size: 22rpx;
  margin-top:15rpx;
  padding: 0 10rpx;
  color: #ccc;
}
.dish-info .dish-maker {
  float:left;
}
.dish-info .dish-maker image,
.dish-info .dish-like image {
  height: 40rpx;
  width: 40rpx;
  vertical-align: text-bottom;
  margin-right: 10rpx;
  border-radius: 50%;
  background-color: rgb(227, 227, 227);
}
.dish-info .dish-like image {
  background-color: #fff;
  margin-right: 5rpx;
}
.dish-info .dish-like {
  float: right;
}