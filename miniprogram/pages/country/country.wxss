.country-view {
  width:100%;
  height:90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 1rpx solid rgb(233, 227, 227);
  padding-left: 20rpx;
}
.country-view view {
  font-size: 28rpx;
  color: rgb(145, 144, 144);
}
.text-view {
  margin:0 20rpx;
}