.main {
  position: relative;
}
.block {
  position: absolute;
  z-index: 200;
  width:100%;
  height:950rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* background-color: rgba(214, 212, 209,.3); */
}
.block .block-content {
  width:400rpx;
  height:300rpx;
  background-color: #fff;
  position: relative;
  /* border: 2rpx solid #d81e06; */
}
.block .block-content .block-clear {
  position: absolute;
  z-index: 999;
  top:-10rpx;
  right:-10rpx;
  width: 35rpx;
  height:35rpx;
  text-align: center;
  line-height: 35rpx;
  color: #fff;
  border-radius: 50%;
  background-color: #d81e06;
}
.block .block-content .block-food {
  margin-top: 20rpx;
  margin-left: 10rpx;
  width: 100%;
  height:90rpx;
  display: flex;
}
.block .block-content .block-count {
  margin-left: 10rpx;
  width: 100%;
  height:90rpx;
  display: flex;
}
.block .block-content .block-ensure {
  width: 100%;
  height:100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.block .block-content .block-ensure button {
  height: 60rpx;
  width: 180rpx;
  margin:0;
  padding: 0;
  line-height: 60rpx;
  border-radius: 30rpx;
  border:3rpx solid #d81e06;
}
.block .block-content text {
  display: inline-block;
  height:60rpx;
  width:100rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  border:3rpx solid #d81e06;
}
.block .block-content input {
  margin: 20rpx 20rpx 0 10rpx;
  padding: 0 5rpx;
  flex: 1;
  border-bottom: 1rpx solid #ccc;
}
page {
  background-color: #fff;
}
.addPro {
  /* background-color: rgb(221, 216, 216); */
  margin-top: 50rpx;
}
.dish-name>text,
.dish-pic>text,
.dish-type>text,
.dish-cook>text,
.steps>text {
  display: inline-block;
  height:80rpx;
  width:100%;
  background-color: rgb(221, 216, 216);
  line-height: 80rpx;
  color: rgb(131, 128, 128);
  padding-left: 20rpx;
}
.dish-name .limit {
  font-size: 22rpx;
}
.dish-name>input {
  width:100%;
  height:70rpx;
  padding-left: 20rpx;
}
.ph {
  font-size: 27rpx;
  color:rgb(218, 215, 215);
}
.step-pic {
  height:350rpx;
  width:100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(216, 213, 213, 0.3);
  color:#ccc;
  position: relative;
}
.step-pic image {
  height:100%;
  width:100%;
}
.step-pic .change {
  position: absolute;
  top:10rpx;
  right:15rpx;
  color:#fff;
  background-color: rgba(179, 176, 176, 0.3);
  font-size: 26rpx;
  width:80rpx;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 20rpx;
}
.dish-type {
  position: relative;
}
.dish-type .addType {
  position: absolute;
  top:8rpx;
  right:10rpx;
  height:60rpx;
  width:60rpx;
  /* background-color: rgba(179, 176, 176, 0.3);
  border-radius: 50%; */
  line-height: 60rpx;
  text-align: center;
  color: rgb(177, 176, 176);
  font-size: 36rpx;
  margin: 5rpx;
}
.dish-type .showType {
  width:100%;
  height:70rpx;
  background-color: #fff;
  line-height: 70rpx;
  padding-left: 20rpx;
}
.dish-cook{
  position: relative;
  margin-bottom: 5rpx;
}
.dish-cook .addFood {
  position: absolute;
  z-index:100;
  font-size: 36rpx;
  height: 30rpx;
  width: 30rpx;
  top:0;
  right:50rpx;
  color: rgb(177, 176, 176);
}
.FoodList {
  width: 100%;
  height:50rpx;
  display: flex;
  margin-top: 5rpx;
  margin-left: 20rpx;
  color: rgb(131, 128, 128);
  font-size: 27rpx;
}
.FoodList>text:first-child {
  flex:5;
}
.FoodList>text:nth-of-type(2) {
  flex:4;
}
.FoodList>text:last-child {
  flex:1;
}
.steps>text {
  position: relative;
}
.steps>text .more {
  position: absolute;
  top:0rpx;
  right:30rpx;
  color: rgb(177, 176, 176);
}
.picList {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 120rpx;
}
.picList .stepImg {
  width:170rpx;
  height:120rpx;
  position: relative;
  margin-top: 10rpx;
  margin-left: 13rpx;
}
.picList .stepImg image {
  height: 100%;
  width:100%;
}
.picList .stepImg .numImg {
  position: absolute;
  bottom:3rpx;
  right:3rpx;
  height:30rpx;
  width:30rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 50%;
  background-color: rgba(31, 30, 30, 0.3);
  font-size: 22rpx;
  color: rgb(233, 229, 229);
}
.submit {
  position: fixed;
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
  padding: 0;
  height:80rpx;
  width:350rpx;
  line-height: 80rpx;
  background-color: #ee5c45;
  border-radius: 20rpx;
  color: #fff;
}
.login-condition {
  width:100%;
  height:1000rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.login-condition image {
  width:200rpx;
}
.login-condition .show-word {
  color:#8a8a8a;
  font-size: 30rpx;
}
.canvasId{
	position: absolute;
	top:-9999rpx;
	left: -9999rpx;
	z-index: 0;
	width: 690rpx;
	height: 690rpx;
}