.search-box {
  position: relative;
  width: 90%;
  height:70rpx;
  margin: 15rpx auto;
  /* margin-top: 15rpx; */
  display: flex;
}
.search-box icon {
  position: absolute;
  top:15rpx;
  left:20rpx;
}
.search-box .search {
  width: 100%;
  height:100%;
  padding-left: 80rpx;
  border: 1rpx solid rgb(63, 63, 63);
  border-radius: 30rpx;
  box-shadow: 0 0 10rpx #b6b3b3;
  background-color: #fff;
}
.search-box .phClass{
  font-size: 27rpx;
}
.menulist {
  background-color: #fff;
  margin-bottom: 15rpx;
}
.menulist .menu-title {
  font-weight: 700;
  font-size: 40rpx;
  width:100%;
  margin:0 10rpx;
  padding: 20rpx 0;
}
.menulist .menu-content view {
  float: left;
  height:200rpx;
  width:33.33%;
  margin-top: 15rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.menulist .menu-content view image {
  height:180rpx;
  width:180rpx;
  border-radius: 50%;
  background-color: rgb(227, 227, 227);
}
.menulist .menu-content view .menuName {
  width: 100%;
  height:60rpx;
  color: #6f6c6c;
  font-size: 28rpx;
}
